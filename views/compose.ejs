<%- include('partials/header'); %>
<h1>Compose</h1>
<form class="" action="/blogpub" method="post">
  <div class="form-group">
    <label for="blogPostTitle">Title</label>
    <input type="text" class="form-control" id="blogPostTitle" name="blogPostTitle" placeholder="">
  </div>
  <div class="form-group">
    <label for="blogPostContent">Post</label>
    <textarea class="form-control" id="blogPostContent" name="blogPostContent" rows="5"></textarea>
  </div>
  <button type="submit" class="btn btn-primary">Publish</button>
</form>
<%- include('partials/footer'); %>
